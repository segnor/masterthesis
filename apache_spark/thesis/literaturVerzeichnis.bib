@MISC{studie:bidama,
  author = {Markl, Volker and Löser, Alexander and Hoeren, Thomas and Krcmar,
	Helmut and Hemsen, Holger and Schermann, Michael and Gottlieb, Matthias
	and Buchmüller, Christoph and Uecker, Philip and Bitter, Till},
  title = {Innovationspotenzialanalyse für die neuen Technologien für das Verwalten
	und Analysieren von großen Datenmengen (Big Data Management)},
  year = {2013},
  abstract = {Durch die Digitalisierung von Wirtschaft und Gesellschaft ist ein
	rasantes Anwachsen von Datenbeständen zu beobachten. In fast allen
	Unternehmens sowie Wissenschaftsbereichen werden bereits heute schon
	Unmengen an Daten erzeugt, deren Größe, Erfassungsgeschwindigkeit
	oder Heterogenität die Fähigkeiten gängiger Datenbanksoftwareprodukte
	zur Verwaltung und zur Analyse übersteigt. Dieses Phänomen, welches
	unter dem Schlagwort „Big Data“ popularisiert wurde, stellt eine
	große Chance für Unternehmen, Wissenschaft und Gesellschaft dar.
	Allerdings ergibt sich aufgrund der neuen Komplexität der Daten und
	Analysen eine Vielzahl an Herausforderungen technischer, wirtschaftlicher
	und rechtlicher Natur. Diese Studie analysiert die Chancen und Herausforderungen
	von Big Data insbesondere im Hinblick auf eine nachhaltige Wettbewerbsfähigkeit
	Deutschlands.},
  file = {:anhangLiteratur\\StudieBiDaMa.pdf:PDF},
  timestamp = {2014.06.02},
  url = {http://www.dima.tu-berlin.de/fileadmin/fg131/Publikation/BDM_Studie/StudieBiDaMa-farbe-druck-v2xx.pdf}
}

